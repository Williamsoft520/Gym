<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <AssemblyName>Williamsoft.Gym</AssemblyName>
    <RootNamespace>System</RootNamespace>
    <Version>3.0.1</Version>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>周大侠</Authors>
    <Company>Williamsoft</Company>
    <Product>Williamsoft.Gym</Product>
    <Description>easy use,easy stronger</Description>
    <Copyright>Williamsoft © 周大侠 2017</Copyright>
    <PackageTags>extension wrapper library</PackageTags>
    <PackageId>Gym</PackageId>
    <AssemblyVersion>3.0.1</AssemblyVersion>
    <FileVersion>3.0.1642.3251</FileVersion>
    <PrimaryVersionType>AssemblyVersionAttribute</PrimaryVersionType>
    <UpdateAssemblyVersion>True</UpdateAssemblyVersion>
    <UpdateAssemblyFileVersion>True</UpdateAssemblyFileVersion>
    <UpdateAssemblyInfoVersion>True</UpdateAssemblyInfoVersion>
    <AssemblyVersionSettings>None.None.None.None</AssemblyVersionSettings>
    <AssemblyFileVersionSettings>None.None.TimeStamp.DeltaDayStamp</AssemblyFileVersionSettings>
    <AssemblyInfoVersionSettings>None.None.None.Increment</AssemblyInfoVersionSettings>
    <RepositoryUrl></RepositoryUrl>
    <RepositoryType>github</RepositoryType>
    <PackageReleaseNotes>transfer project from Framework4.5 to .Net Standard 1.3
[new] Add HttpExtension, user can push http client easily</PackageReleaseNotes>
    <PackageProjectUrl></PackageProjectUrl>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netstandard1.3\Williamsoft.Gym.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Release\netstandard1.3\Williamsoft.Gym.xml</DocumentationFile>
    <FileAlignment>1024</FileAlignment>
    <DebugType>none</DebugType>
    <DebugSymbols>False</DebugSymbols>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Net.Requests" Version="4.3.0" />
  </ItemGroup>

</Project>